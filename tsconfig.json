{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "noImplicitAny": true,
      "resolveJsonModule": true,
          
    },
    "include": ["src/**/*"]
  }